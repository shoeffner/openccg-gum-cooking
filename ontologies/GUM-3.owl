        <rdfs:subClassOf rdf:resource="#StativeQuality"/>
        <owl:disjointWith rdf:resource="#StatusQuality"/>
        <rdfs:comment rdf:datatype="&xsd;string"
            >These are qualities that are sensed or measured by conscious beings. For example, this category would include qualities of age (young, old), of weight (light, heavy), price (expensive, cheap), etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.ontospace.uni-bremen.de/ontology/stable/GUM-3.owl#Signification -->

